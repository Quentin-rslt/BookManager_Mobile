{
  "expo": {
    "name": "Mobile_App",
    "slug": "Mobile_App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_BookManager.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo_BookManager.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_BookManager.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/logo_BookManager.png"
    },
    "androidStatusBar": {
      "backgroundColor": "#1f1616",
      "translucent": false
    }
  }
}
